@prefix dcat: <http://www.w3.org/ns/dcat#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix language: <http://id.loc.gov/vocabulary/iso639-1/>.
@prefix loc: <http://localhost/>.
@prefix dc: <http://purl.org/biosemantics-lumc/ontologies/dcat-extension/>.
@prefix c: <http://localhost/catalog/>.
@prefix ed: <http://edamontology.org/>.
@prefix ent: <http://www.wikidata.org/entity/>.

loc:new
    a <{{&resourceType}}>, dcat:Resource;
    dct:description "{{description}}";
    dct:hasVersion "1";
    dct:isPartOf <{{&catalog_url}}>;
    dct:language language:en;
    dct:license <http://rdflicense.appspot.com/rdflicense/cc-by-nc-nd3.0>;
    dct:publisher [ a foaf:Agent; foaf:name "{{publisher}}" ];
    dct:title "{{title}}";
    dcat:keyword  {{&keyword}};
    dcat:theme {{&theme}}.